{
  "private": true,
  "name": "sub1",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "resolutions": {
    "qiankun": "2.4.5"
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^4.18.6",
    "qiankun": "2.4.5",
    "react": "^17.x",
    "react-dom": "17.x",
    "redux-persist": "^6.0.0",
    "slash2": "^2.0.0",
    "umi": "^3.5.43"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-esbuild": "^1.4.2",
    "@umijs/plugin-qiankun": "^2.22.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.43",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
